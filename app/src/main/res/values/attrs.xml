<resources>

    <!-- Declare custom theme attributes that allow changing which styles are
         used for button bars depending on the API level.
         ?android:attr/buttonBarStyle is new as of API 11 so this is
         necessary to support previous API levels. -->
    <attr name="colorFade" format="color" />
    <attr name="text_color_primary" format="color" />
    <attr name="text_color_secondary" format="color" />
    <attr name="background_color_bottom" format="color" />
    <attr name="background_color_second" format="color" />
    <attr name="colorTransparent" format="color" />
    <attr name="background_color_bottom_as_tint" format="color" />
    <attr name="background_icon_color_bottom" format="color"/>
    <attr name="background_icon_color_second" format="color"/>
    <attr name="drawer_header_background" format="reference" />
    <attr name="drawer_header_buttons_background" format="reference" />
    <attr name="color_primary_fade_background" format="reference" />



</resources>

