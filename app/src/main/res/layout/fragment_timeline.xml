<?xml version="1.0" encoding="utf-8"?>


<com.stupidtree.hita.views.pullextend.PullExtendLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/pull_extend"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--头部-->
    <com.stupidtree.hita.views.pullextend.ExtendListHeader
        android:id="@+id/extend_header"
        android:layout_width="match_parent"
        android:layout_height="220dp">

    </com.stupidtree.hita.views.pullextend.ExtendListHeader>

    <com.stupidtree.hita.views.pullextend.mRecyclerView
        android:id="@+id/timelinelist"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layoutAnimation="@anim/recycler_layout_animation_fade"
        android:overScrollMode="never">

    </com.stupidtree.hita.views.pullextend.mRecyclerView>

    <!--底部区域-->
</com.stupidtree.hita.views.pullextend.PullExtendLayout>




