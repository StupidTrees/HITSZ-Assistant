<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:alpha="0.9"    android:orientation="vertical"    android:paddingStart="24dp"    android:paddingTop="16dp"    android:paddingEnd="16dp"    android:paddingBottom="16dp">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_marginBottom="16dp"        android:layout_weight="1"        android:orientation="horizontal">        <ImageView            android:layout_width="24dp"            android:layout_height="24dp"            android:layout_gravity="center"            android:layout_marginRight="8dp"            android:tint="?attr/colorPrimary"            app:srcCompat="@drawable/ic_dynamic" />        <TextView            android:id="@+id/tt_dlg_name"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:layout_weight="1"            android:gravity="center|start"            android:text="高等数学B"            android:textColor="@color/material_primary_text"            android:textSize="20sp"            android:transitionName="course_name" />    </LinearLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_marginTop="10dp"        android:layout_marginBottom="10dp"        android:layout_weight="1"        android:gravity="center"        android:orientation="horizontal">        <ImageView            android:layout_width="18dp"            android:layout_height="18dp"            android:layout_gravity="top"            android:layout_marginLeft="0dp"            android:layout_marginRight="16dp"            android:layout_marginBottom="20dp"            android:scaleType="fitCenter"            android:tint="@color/material_secondary_text"            app:srcCompat="@drawable/ic_dlg_tag2" />        <LinearLayout            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_weight="1"            android:orientation="vertical">            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_weight="1"                android:gravity="left|center"                android:text="标签1"                android:textColor="#757575"                android:textSize="12sp" />            <TextView                android:id="@+id/tt_dlg_value1"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:singleLine="false"                android:text="电工\n大雾\n数据结构"                android:textColor="#212121"                android:textSize="18sp" />        </LinearLayout>    </LinearLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginTop="10dp"        android:layout_marginBottom="10dp"        android:layout_weight="1"        android:gravity="center"        android:orientation="horizontal">        <ImageView            android:layout_width="18dp"            android:layout_height="18dp"            android:layout_gravity="top"            android:layout_marginLeft="0dp"            android:layout_marginRight="16dp"            android:layout_marginBottom="20dp"            android:scaleType="fitCenter"            android:tint="@color/material_secondary_text"            app:srcCompat="@drawable/ic_dlg_tag2" />        <LinearLayout            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_weight="1"            android:orientation="vertical">            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_weight="1"                android:gravity="left|center"                android:text="标签2"                android:textColor="#757575"                android:textSize="12sp" />            <TextView                android:id="@+id/tt_dlg_value2"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_weight="1"                android:gravity="center"                android:singleLine="true"                android:text="T3502"                android:textColor="#212121"                android:textSize="18sp" />        </LinearLayout>    </LinearLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_marginTop="10dp"        android:layout_marginBottom="10dp"        android:layout_weight="1"        android:gravity="center"        android:orientation="horizontal">        <ImageView            android:layout_width="18dp"            android:layout_height="18dp"            android:layout_gravity="top"            android:layout_marginLeft="0dp"            android:layout_marginRight="16dp"            android:layout_marginBottom="20dp"            android:scaleType="fitCenter"            android:tint="@color/material_secondary_text"            app:srcCompat="@drawable/ic_dlg_tt_duration" />        <LinearLayout            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_weight="1"            android:orientation="vertical">            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_weight="1"                android:gravity="center"                android:text="时间"                android:textColor="#757575"                android:textSize="12sp" />            <TextView                android:id="@+id/tt_dlg_value3"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_weight="1"                android:gravity="left|center"                android:singleLine="true"                android:text="10:30-11:00"                android:textColor="#212121"                android:textSize="18sp" />        </LinearLayout>    </LinearLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_marginTop="10dp"        android:layout_marginBottom="10dp"        android:layout_weight="1"        android:gravity="center"        android:orientation="horizontal">        <ImageView            android:layout_width="18dp"            android:layout_height="18dp"            android:layout_gravity="top"            android:layout_marginLeft="0dp"            android:layout_marginRight="16dp"            android:layout_marginBottom="20dp"            android:scaleType="fitCenter"            android:tint="@color/material_secondary_text"            app:srcCompat="@drawable/ic_dlg_timetable_date" />        <LinearLayout            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_weight="1"            android:orientation="vertical">            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_weight="1"                android:gravity="center"                android:text="日期"                android:textColor="#757575"                android:textSize="12sp" />            <TextView                android:id="@+id/tt_dlg_date"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_weight="1"                android:gravity="left|center"                android:singleLine="true"                android:text="3月24日(第三周星期日）"                android:textColor="#212121"                android:textSize="18sp" />        </LinearLayout>    </LinearLayout></LinearLayout>