<?xml version="1.0" encoding="UTF-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:id="@+id/recy_item_layout"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:clickable="true"    android:foreground="?attr/selectableItemBackgroundBorderless">    <ImageView        android:id="@+id/chatbot_courselist_icon"        android:layout_width="20dp"        android:layout_height="20dp"        android:layout_gravity="center"        android:layout_marginLeft="12dp"        android:tint="?attr/colorPrimary"        app:srcCompat="@drawable/ic_label" />    <TextView        android:id="@+id/tasklist_name"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:clickable="false"        android:descendantFocusability="blocksDescendants"        android:gravity="center_vertical"        android:minHeight="?android:attr/listPreferredItemHeightSmall"        android:paddingStart="?android:attr/listPreferredItemPaddingStart"        android:paddingEnd="?android:attr/listPreferredItemPaddingEnd"        android:singleLine="true"        android:text="2018课表"        android:textAppearance="?android:attr/textAppearanceListItemSmall"        android:textSize="12sp" /></LinearLayout>